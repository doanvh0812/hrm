<odoo>
    <record id="module_hrm" model="ir.module.category">
        <field name="name">HRMS</field>
        <field name="sequence">0</field>
    </record>

    <record id="group_users" model="res.groups">
        <field name="name">Nhân viên</field>
        <field name="category_id" ref="module_hrm"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_manager" model="res.groups">
        <field name="name">Quản trị viên</field>
        <field name="category_id" ref="module_hrm"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_administrator" model="res.groups">
        <field name="name">Cấu hình</field>
        <field name="category_id" ref="module_hrm"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

</odoo>