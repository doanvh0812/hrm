<odoo>
    <record id="view_see_image_form" model="ir.ui.view">
        <field name="name">view_see_image_form</field>
        <field name="model">see.image</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="upload_image" widget="image"/>
                        <field name="upload_image_ids"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_see_image_form" model="ir.ui.view">
        <field name="name">view_see_image_form</field>
        <field name="model">see.image</field>
        <field name="arch" type="xml">
            <field name="id"/>
        </field>
    </record>
</odoo>